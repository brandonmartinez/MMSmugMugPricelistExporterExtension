{
  "manifest_version": 2,

  "name": "SmugMug Pricelist Exporter",
  "description": "Export your SmugMug pricelists to various formats.",
  "version": "0.9",
  
  "content_scripts": [
    {
      "matches": ["https://secure.smugmug.com/sales/pricing/edit/*"],
      "js": ["jquery.js", "mm.smugmug.pricelist.exporter.extension.js"]
    }
  ]
}